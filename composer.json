{
    "name": "panda/api",
    "description": "The Panda framework api template.",
    "keywords": [
        "framework",
        "panda"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.0",
        "panda/framework": "^2.0",
        "firebase/php-jwt": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
